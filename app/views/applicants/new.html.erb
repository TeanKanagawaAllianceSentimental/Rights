<div class="container">
	<div class="row">
		<div class="col-md-12 text-center"">
			<div class="progress">
			  <div class="progress-bar" role="progressbar" style="width: 30%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
			</div>
			<h2>申請者情報入力フォーム</h2><br>
			<%= form_for (@applicant) do |form| %>
          <div class="form-group">
            <h4>法人名 団体名</h4>
            <%= f.text_field :applicant_organisation_name %>
          </div>
          <div class="form-group">
            <h4>法人名 団体名（ふりがな）</h4>
            <%#= f.text_field :applicant_organisation_name %>
          </div>
          <h4>代表者名</h4>
            <%= f.text_field :representative_name %>
          </div>
          <div class="form-group">
            <h4>代表者名（ふりがな）</h4>
            <%#= f.text_field :representative_name %>
          </div>
          <div class="form-group">
            <h4>郵便番号</h4>
            <%= f.text_field :applicant_postal_code %>
          </div>
          <div class="form-group">
            <h4>住所</h4>
            <%= f.text_field :applicant_address %>
          </div>
          <div class="form-group">
            <h4>電話番号</h4>
            <%= f.text_field :applicant_telephone %>
          </div>
          <div class="form-group">
            <h4>FAX</h4>
            <%= f.text_field :applicant_fax %>
          </div>
          <div class="form-group">
            <h4>ご担当者名</h4>
            <%= f.text_field :contact_person %>
          </div>
          <div class="form-group">
            <h4>ご担当者名（ふりがな）</h4>
            <%= f.text_field :contact_person %>
          </div>
          <div class="form-group">
            <h4>部署名</h4>
            <%= f.text_field :applicant_department %>
          </div>
          <div class="form-group">
            <h4>Email</h4>
            <%= f.email_field :contact_person_email %>
          </div>
          <div class="form-group">
            <h4>日中のご連絡先（上記と異なる場合）</h4>
            <%= f.text_field :contact_person_mobilephone %>
          </div>
          <div class="form-group">
            <%= f.submit "更新", class:"btn btn-md btn-primary" %>
          </div>
        <br>
      </div>
    </div>
</div>

